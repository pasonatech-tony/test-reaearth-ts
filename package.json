{
  "name": "my-reearth-spaceid",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/reearth-plugin-spaceid.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "vite -c vite.config.spaceid-web.ts",
    "build": "run-s typecheck clean build:web build:plugin",
    "build:plugin": "vite build -c vite.config.spaceid.ts",
    "build:web": "vite build -c vite.config.spaceid-web.ts",
    "package": "run-s build zip",
    "typecheck": "tsc",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "type": "tsc",
    "zip": "node ./scripts/zip.mjs",
    "gen": "node ./scripts/gen.mjs",
    "clean": "del dist"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/leaflet": "1.9.0",
    "@types/lodash": "4.14.191",
    "@types/node": "18.11.18",
    "@types/papaparse": "5.3.7",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/remarkable": "2.0.3",
    "@types/styled-components": "5.1.26",
    "@types/tinycolor2": "^1.4.3",
    "@vitejs/plugin-react": "3.0.1",
    "antd": "4.24.7",
    "archiver": "5.3.1",
    "del-cli": "5.0.0",
    "eslint": "8.31.0",
    "eslint-config-reearth": "0.2.1",
    "jsdom": "21.0.0",
    "less": "4.1.3",
    "less-loader": "11.1.0",
    "msw": "0.49.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.2",
    "rollup-plugin-visualizer": "5.9.0",
    "styled-components": "5.3.6",
    "typescript": "4.9.4",
    "typescript-styled-plugin": "0.18.2",
    "vite": "4.0.4",
    "vite-plugin-cdn-import": "0.3.5",
    "vite-plugin-dts": "1.7.1",
    "vite-plugin-singlefile": "0.13.2",
    "vite-plugin-svgr": "2.4.0",
    "vite-tsconfig-paths": "4.0.5",
    "vitest": "0.27.0",
    "yml": "1.0.0"
