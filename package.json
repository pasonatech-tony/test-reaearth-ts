    "papaparse": "5.3.2",
    "react": "18.2.0",
    "react-accessible-accordion": "5.0.0",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.2.0",
    "react-json-view": "1.21.3",
    "remarkable": "2.0.1",
    "tinycolor2": "1.6.0"
  },
  "msw": {
    "workerDirectory": "web/mock"
  }
